page {
    background-color: #f1f1f1;
}
.project_main {
    padding: 50rpx 30rpx;
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    flex-wrap: wrap;
}
.project_main .project_item {
    width: 330rpx;
    box-sizing: border-box;
    margin-bottom: 30rpx;
    background-color: #fff;
}
.project_main .project_item .project_pic {
    width: 100%;
    height: 190rpx;
    overflow: hidden;
    background: #f5f2f2;
}
.project_main .project_item .project_pic image {
    width: 100%;
}
.project_main .project_item .project_text {
    font-size: 28rpx;
    color: #888888;
    padding: 10rpx;
    line-height: 34rpx;
}